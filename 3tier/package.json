{
  "name": "3tier",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "update": "yarn upgrade -L",
    "destroy": "yarn build && cdk destroy --require-approval broadening",
    "debug": "tsc && cdk deploy -v",
